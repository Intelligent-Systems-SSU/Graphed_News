{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "format": "prettier --ignore-path .prettierignore --check .",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "proto:generate": "mkdir -p app/generated && protoc -I=../protos --ts_out=./app/generated ../protos/*.proto",
    "create-migration": "npx wrangler d1 migrations create graphed-news-db",
    "diff-migration": "npx prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script --output ",
    "run-migration": "npx wrangler d1 migrations apply graphed-news-db"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "@prisma/adapter-d1": "^6.2.1",
    "@prisma/client": "^6.2.1",
    "@remix-run/cloudflare": "^2.15.3",
    "@remix-run/cloudflare-pages": "^2.15.3",
    "@remix-run/react": "^2.15.3",
    "google-protobuf": "^3.21.4",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250407.0",
    "@remix-run/dev": "^2.15.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "grpc-tools": "^1.13.0",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "prisma": "^6.2.1",
    "protoc-gen-ts": "^0.8.7",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "4.8.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
